<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Sign_in_up">

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:contentDescription="1"
        android:scaleX="1.06"
        android:scaleY="1.06"
        app:srcCompat="@drawable/sign_in_up"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="0dp" />

    <ImageButton
        android:id="@+id/doc_signUp_first"
        android:layout_width="270dp"
        android:layout_height="40dp"
        android:layout_marginBottom="52dp"
        android:alpha="1"
        android:background="#000000"
        android:backgroundTint="#000000"
        android:scaleX="1"
        android:scaleY="1"
        android:translationY="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/doc_logIn_first"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/doc_logIn_first"
        app:layout_constraintTop_toBottomOf="@+id/doc_logIn_first"
        app:layout_constraintVertical_bias="0.964"
        app:srcCompat="@drawable/sign_up_button3"
        app:tint="#000000" />

    <ImageButton
        android:id="@+id/doc_logIn_first"
        android:layout_width="280dp"
        android:layout_height="40dp"
        android:layout_marginStart="72dp"
        android:layout_marginTop="248dp"
        android:alpha="1"
        android:backgroundTint="#00FFFFFF"
        android:scaleX="1"
        android:scaleY="1"
        android:translationY="3dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/sign_up_button3" />

    <ImageButton
        android:id="@+id/pat_signUp_first"
        android:layout_width="270dp"
        android:layout_height="40dp"
        android:layout_marginEnd="104dp"
        android:layout_marginBottom="52dp"
        android:alpha="1"
        android:backgroundTint="#0B0000"
        android:contentDescription="21"
        android:scaleX="1"
        android:scaleY="1"
        android:translationX="40dp"
        android:translationY="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/pat_logIn_first"
        app:layout_constraintTop_toBottomOf="@+id/pat_logIn_first"
        app:layout_constraintVertical_bias="0.958"
        app:srcCompat="@drawable/sign_up_button3"
        app:tint="#000000" />

    <ImageButton
        android:id="@+id/pat_logIn_first"
        android:layout_width="270dp"
        android:layout_height="40dp"
        android:layout_marginTop="248dp"
        android:layout_marginEnd="88dp"
        android:alpha="1"
        android:backgroundTint="#00FFFFFF"
        android:scaleX="1"
        android:scaleY="1"
        android:translationX="30dp"
        android:translationY="3dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/doc_logIn_first"
        app:layout_constraintTop_toTopOf="@+id/imageView7"
        app:srcCompat="@drawable/sign_up_button3" />

</androidx.constraintlayout.widget.ConstraintLayout>